server {
   listen         80 default_server;
   listen         [::]:80 default_server;
   server_name    localhost;
   root           /usr/share/nginx/html;
   location / {
       proxy_pass http://127.0.0.1:8000;
       proxy_http_version 1.1;
   }
   location /restaurant {
       proxy_pass http://127.0.0.1:8001;
       proxy_http_version 1.1;
   }
}